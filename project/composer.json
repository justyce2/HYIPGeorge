{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "authorizenet/authorizenet": "2.0.2",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "brian2694/laravel-toastr": "^5.56",
        "cartalyst/stripe-laravel": "^13.0",
        "coingate/coingate-php": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "instamojo/instamojo-php": "^0.4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "mews/purifier": "^3.3",
        "mollie/laravel-mollie": "^2.10",
        "nexmo/laravel": "^2.4",
        "paypal/rest-api-sdk-php": "*",
        "phpmailer/phpmailer": "^6.5",
        "razorpay/razorpay": "^2.5",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-sitemap": "^5.8.0",
        "twilio/sdk": "^6.34",
        "yajra/laravel-datatables-oracle": "~9.0",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Markury\\": "vendor/markury/src/Adapter"
        },
        "files": ["app/Helpers/helper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
